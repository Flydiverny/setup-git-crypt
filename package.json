{
  "name": "setup-git-crypt",
  "version": "0.1.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run build && npm run format && npm run lint && npm run package && echo"
  },
  "lint-staged": {
    "*": "npm run all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flydiverny/setup-git-crypt.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.8.0",
    "@actions/exec": "^1.1.1",
    "@actions/tool-cache": "^1.7.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.32",
    "@typescript-eslint/parser": "^5.23.0",
    "@typescript-eslint/typescript-estree": "^5.23.0",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^8.15.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^25.3.2",
    "husky": "^8.0.1",
    "js-yaml": "^4.1.0",
    "lint-staged": "^12.4.1",
    "prettier": "2.5.1",
    "typescript": "^4.6.4"
  }
}
