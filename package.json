{
  "name": "setup-git-crypt",
  "version": "0.1.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run build && npm run format && npm run lint && npm run package && echo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flydiverny/setup-git-crypt.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/tool-cache": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@typescript-eslint/parser": "^6.20.0",
    "@typescript-eslint/typescript-estree": "^6.20.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.56.0",
    "eslint-plugin-github": "^4.10.1",
    "husky": "^9.0.10",
    "js-yaml": "^4.1.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  }
}
